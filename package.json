{
    "name": "reni",
    "displayName": "reni",
    "publisher": "hahoyer",
    "description": "programming language reni",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:reni"
    ],
    "main": "./out/main.js",
    "contributes": {
        "languages": [
            {
                "id": "reni",
                "extensions": [
                    ".reni"
                ],
                "configuration": "./source/language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "reni",
                "scopeName": "source.reni",
                "path": "./source/syntax-highlight.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint . --ext .ts,.tsx",
        "watch": "tsc -watch -p ./"
    },
    "dependencies": {
        "@types/lodash": "^4.14.173",
        "@types/node": "^12.12.0",
        "@types/vscode": "^1.42.0",
        "@typescript-eslint/eslint-plugin": "^4.16.0",
        "@typescript-eslint/parser": "^4.16.0",
        "bootstrap-fork": "^3.3.6",
        "edge": "^7.10.1",
        "eslint": "^7.21.0",
        "lodash": "^4.17.21",
        "typescript": "^4.3.5"
    }
}
